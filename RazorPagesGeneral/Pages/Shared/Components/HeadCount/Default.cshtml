@model IEnumerable<DeptHeadCount>

 <table class="table table-hover rounded-pill">
     <thead class="table-light">
         <tr>
             <th>Отдел</th>
             <th>Сотрудники</th>
         </tr>
     </thead>
     <tbody>
         @foreach (var item in Model)
         {
             <tr>
                 <td>@item.Department</td>
                 <td>@item.Count</td>
             </tr>
         }
     </tbody>
 </table>